#!/bin/bash

source "`dirname "$0"`"/elasticsearch-env

# include dump module jars in classpath
ES_CLASSPATH="$ES_CLASSPATH:$ES_HOME/modules/dump/*"

exec \
  "$JAVA" \
  $ES_JAVA_OPTS \
  -Des.path.home="$ES_HOME" \
  -Des.path.conf="$ES_PATH_CONF" \
  -cp "$ES_CLASSPATH" \
  org.elasticsearch.dump.DumpToolCli \
  "$@"
